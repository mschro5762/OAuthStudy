{
  "server": {
    "address": ":8080"
  },
  "clientRegistry": {
    "secretLength": 64,
    "repoFilePath": "./filerepos/clientrepo.json"
  },
  "users": {
    "repoFilePath": "./filerepos/usersrepo.json"
  },
  "tokens": {
    "issuerUri": "https://example.com",
    "authzCodeCrypto": {
      "name": "AES256-GCM",
      "keyFile": "./cryptoKeys/aes.1.key"
    },
    "authzCodeSigning": {
      "name": "HMAC+SHA256",
      "keyfile": "./cryptokeys/sha.1.key"
    },
    "authzCodeTtl": "30s",
    "accessTokenCrypto": {
      "name": "AES256-GCM",
      "keyFile": "./cryptoKeys/aes.1.key"
    },
    "accessTokenSigning": {
      "name": "HMAC+SHA256",
      "keyfile": "./cryptokeys/sha.1.key"
    },
   "accessTokenTtl": "30m"
  }
}
